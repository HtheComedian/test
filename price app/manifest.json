{
    "manifest_version": 3,
    "name": "eBay Price Checker",
    "version": "1.0",
    "description": "Check which eBay items are priced over $20.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  